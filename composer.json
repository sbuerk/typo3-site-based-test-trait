{
    "name": "sbuerk/typo3-site-based-test-trait",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "description": "Provides modified TYPO3 SiteBasedTestTrait and TF FunctionalTestCase.",
    "authors": [
        {
            "name": "Stefan BÃ¼rk",
            "email": "stefan@buerk.tech"
        }
    ],
    "keywords": [
        "typo3",
        "testing"
    ],
    "config": {
        "allow-plugins": true,
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "preferred-install": {
            "typo3/cms-core": "source",
            "typo3/cms-frontend": "source",
            "*": "dist"
        },
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "sbuerk_testing",
            "web-dir": ".Build/Web"
        },
        "branch-alias": {
            "dev-main": "3.x-dev"
        }
    },
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4",
        "ext-json": "*",
        "typo3/cms-core": "13.*.*@dev"
    },
    "require-dev": {
        "bnf/phpstan-psr-container": "^1.1",
        "friendsofphp/php-cs-fixer": "^3.75.0",
        "nikic/php-parser": "^5.4.0",
        "phpstan/phpstan": "^2.1.11",
        "phpunit/phpunit": "^10.5.45",
        "typo3/cms-backend": "13.*.*@dev",
        "typo3/cms-belog": "13.*.*@dev",
        "typo3/cms-beuser": "13.*.*@dev",
        "typo3/cms-extbase": "13.*.*@dev",
        "typo3/cms-fluid": "13.*.*@dev",
        "typo3/cms-fluid-styled-content": "13.*.*@dev",
        "typo3/cms-frontend": "13.*.*@dev",
        "typo3/cms-install": "13.*.*@dev",
        "typo3/cms-lowlevel": "13.*.*@dev",
        "typo3/cms-setup": "13.*.*@dev",
        "typo3/cms-tstemplate": "13.*.*@dev",
        "typo3/minimal": "^13",
        "typo3/testing-framework": "^8.2.7"
    },
    "autoload": {
        "psr-4": {
            "SBUERK\\TYPO3\\Testing\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SBUERK\\TYPO3\\Testing\\Tests\\": "Tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
